{% if myMatches %}
    {% for day in myMatches %}
        {% filter shortcodes %}   
            [av_heading heading='{{ day.date }}' tag='h2' style='' size='' subheading_active='' subheading_size='15' margin='' margin_sync='true' padding='10' color='' custom_font='' av-medium-font-size-title='' av-small-font-size-title='' av-mini-font-size-title='' av-medium-font-size='' av-small-font-size='' av-mini-font-size='' av_uid='av-jl3st843' custom_class='' admin_preview_bg='']
            [/av_heading] 
            [av_table purpose='tabular' pricing_table_design='avia_pricing_default' pricing_hidden_cells='' caption='' responsive_styling='avia_scrollable_table' av_uid='' custom_class='']
                [av_row row_style='']
                    [av_cell col_style='']Catégorie[/av_cell]
                    [av_cell col_style='']Lieu[/av_cell]
                    [av_cell col_style='']Rencontre[/av_cell]
                    [av_cell col_style='']RDV[/av_cell]
                    [av_cell col_style='']Match[/av_cell]
                [/av_row]
                {% for match in day.content %}
                    [av_row row_style='']
                        [av_cell col_style='']{{ match.domTeam }}[/av_cell]
                        [av_cell col_style='']{{ match.ext }}[/av_cell]
                        [av_cell col_style='']{{ match.extTeam }}[/av_cell]
                        [av_cell col_style='']{{ match.hourRDV }}[/av_cell]
                        [av_cell col_style='']{{ match.hourStart }}[/av_cell]
                    [/av_row]
                {% endfor %}
            [/av_table]
        {% endfilter %}
    {% endfor %}
{% else %}
    {% filter shortcodes %}   
        [av_heading heading='Aucune match trouvé' tag='h2' style='' size='' subheading_active='' subheading_size='15' margin='' margin_sync='true' padding='10' color='' custom_font='' av-medium-font-size-title='' av-small-font-size-title='' av-mini-font-size-title='' av-medium-font-size='' av-small-font-size='' av-mini-font-size='' av_uid='av-jl3st843' custom_class='' admin_preview_bg=''][/av_heading]  
    {% endfilter %}
{% endif %}
